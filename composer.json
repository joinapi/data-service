{
    "name": "joinapi/data-service",
    "description": "Technical: Service",
    "keywords": [
        "JOIN API",
        "laravel",
        "data-service"
    ],
    "homepage": "https://github.com/joinapi/data-service",
    "license": "MIT",
    "authors": [
        {
            "name": "JOIN API",
            "email": "devops@joinapi.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^v11.0",
        "laravel/framework": "^v11.0.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^v1.14",
        "nunomaduro/collision": "^v8.1.1||^v7.10.0",
        "larastan/larastan": "^v2.9",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^v3.0",
        "pestphp/pest-plugin-arch": "^v3.0",
        "pestphp/pest-plugin-laravel": "^v3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "Joinbiz\\": "src/Joinbiz/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joinbiz\\": "src/Joinbiz/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    }
}
